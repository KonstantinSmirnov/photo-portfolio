<%= simple_form_for @instagram, url: admin_instagram_path, html: { autocomplete: 'off' }, remote: true do |f| %>
  <%= f.input :text, input_html: { class: 'textarea-resizable' } %>
  <%= f.submit class: 'btn btn-default-light' %>
  <%= link_to 'Cancel', cancel_admin_instagram_path, class: 'btn btn-default-light', remote: true %>
<% end %>
