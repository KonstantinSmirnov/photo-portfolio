<div id="photo-<%= object.id %>" class="photo col-6 col-sm-6 col-md-4 col-lg-3">
  <%= image_tag object.image.url(:thumb), class: 'img-fluid' %>
  <div class="change-controls">
    <i class="fa fa-arrows"></i>
    <h5><%= object.alt %></h5>
    <div class="buttons">
      <%= link_to 'Edit', edit_admin_project_photo_path(object.project, object), remote: true, class: 'btn btn-photo-dark' %>
      <%= link_to 'Delete', admin_project_photo_path(object.project, object), method: :delete, data: { confirm: 'Are you sure?' }, remote: true, class: 'btn btn-photo-dark' %>
    </div>
  </div>
</div>
